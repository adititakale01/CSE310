import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int a,b;
        Scanner s = new Scanner(System.in);
        a = s.nextInt();
        b = s.nextInt();
        if(a<0 || b<0)
            System.out.print("Invalid Input");
        else
            System.out.print(a+"-"+b);
    }
}
