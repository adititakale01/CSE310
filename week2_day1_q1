import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class W11 {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        String a = s.nextLine();
        int decimal=Integer.parseInt(a,2);
        if(decimal>=65 && decimal<=90)
        {
            char c =(char)decimal;  
            System.out.println(c); 
        }    
        else
        {                
            System.out.print("INVALID PAPER CODE");
        }
    }
}
